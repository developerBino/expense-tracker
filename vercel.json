{
  "buildCommand": "echo 'Static site with serverless functions'",
  "outputDirectory": ".",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "GOOGLE_APPS_SCRIPT_URL": {
      "description": "Google Apps Script deployment URL for accessing Google Sheets"
    }
  }
}
